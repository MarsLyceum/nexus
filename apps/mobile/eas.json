{
    "cli": {
        "version": ">= 7.6.2"
    },
    "build": {
        "development": {
            "env": {
                "EXPO_USE_BRIDGLESS_ARCHITECTURE": "true"
            }
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_USE_BRIDGLESS_ARCHITECTURE": "true"
            }
        },
        "production": {
            "env": {
                "EXPO_USE_BRIDGLESS_ARCHITECTURE": "true"
            }
        },
        "preview2": {
            "android": {
                "gradleCommand": ":app:assembleRelease"
            }
        },
        "preview3": {
            "developmentClient": true
        },
        "preview4": {
            "distribution": "internal"
        },
        "production": {}
    },
    "submit": {
        "production": {}
    }
}