{
    "name": "nexus-mobile",
    "version": "1.0.1",
    "private": true,
    "main": "index.js",
    "scripts": {
        "expo-install": "npx expo install --pnpm",
        "start": "npx expo start",
        "export:web": "npx expo export",
        "export:android": "eas build --platform android",
        "export:ios": "eas build --platform ios",
        "android": "npx expo start --android",
        "ios": "npx expo start --ios",
        "web": "npx expo start --web",
        "lint": "npx eslint .",
        "typecheck": "tsc --noEmit",
        "deploy": "pnpm run export:web && firebase deploy"
    },
    "dependencies": {}
}