{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "jsx": "react-native",
        "target": "es5",
        "module": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"]
}
